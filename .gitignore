# Ignore Python cache and virtual environments
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
venv/
env/
ENV/
.venv

# Ignore Node modules
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore build outputs
dist/
build/
*.egg-info/
.eggs/

# Ignore IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# Ignore environment files
.env
.env.local
.env.*.local
*.env

# Ignore data files (genomes, results)
data/genomes/*
!data/genomes/.gitkeep
data/results/*
!data/results/.gitkeep
data/exports/*
!data/exports/.gitkeep
data/cache/*
!data/cache/.gitkeep

# Ignore logs
logs/*.log
logs/backend/*
!logs/backend/.gitkeep
logs/frontend/*
!logs/frontend/.gitkeep

# Ignore database files
*.db
*.sqlite
*.sqlite3

# Ignore Docker volumes
postgres_data/
redis_data/

# Ignore coverage reports
htmlcov/
.coverage
.coverage.*
coverage.xml
*.cover

# Ignore pytest cache
.pytest_cache/

# Ignore Alembic
alembic/versions/*.pyc

# Ignore OS files
.DS_Store
Thumbs.db
desktop.ini

# Ignore temporary files
*.tmp
*.temp
*.bak
*.swp

# Keep .gitkeep files
!.gitkeep
